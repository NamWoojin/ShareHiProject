<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".main.MainActivity">

    <Button
        android:id="@+id/activity_main_go_send_buttion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="공유하기"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        ></Button>

    <Button
        android:id="@+id/activity_main_go_receive_buttion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="공유받기"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_main_go_send_buttion"
        ></Button>
    <Button
        android:id="@+id/activity_main_go_user_buttion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="계정설정"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_main_go_receive_buttion"
        ></Button>

</androidx.constraintlayout.widget.ConstraintLayout>